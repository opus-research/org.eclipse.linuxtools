<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="org.eclipse.linuxtools.mylyn.osio.rest.repository"
         name="OpenShift.io REST Respository"
         point="org.eclipse.mylyn.tasks.ui.repositories">
      <connectorUi
            brandingIcon="icons/eview16/osio-logo.gif"
            class="org.eclipse.linuxtools.internal.mylyn.osio.rest.ui.OSIORestRepositoryConnectorUI"
            overlayIcon="icons/eview16/overlay-osio.gif">
      </connectorUi>
   </extension>
   <extension
         point="org.eclipse.mylyn.tasks.ui.editors">
      <pageFactory
            class="org.eclipse.linuxtools.internal.mylyn.osio.rest.ui.OSIORestTaskEditorPageFactory"
            id="org.eclipse.mylyn.bugzilla.rest.ui.pageFactory">
      </pageFactory>
   </extension>
</plugin>
